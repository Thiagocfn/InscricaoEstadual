  "require": {
    "php": ">=5.4.16"
  },
  "authors": [
    {
      "name": "Thiago Souza",
      "email": "thiagocfn@msn.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "codacy/coverage": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Thiagocfn\\InscricaoEstadual\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thiagocfn\\InscricaoEstadual\\": "tests/"
    }
  }
}
